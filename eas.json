{
  "cli": {
    "version": ">= 15.0.15",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development",
        "EXPO_USE_HERMES": "true"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_HERMES": "true"
      },
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"
    },
    "production": {
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_HERMES": "true",
        "EXPO_USE_METRO_SERIALIZER": "true"
      },
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
